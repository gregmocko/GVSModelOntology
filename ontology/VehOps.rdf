    <owl:Class rdf:about="http://www.semanticweb.org/eloui/ontologies/2022/1/VehOps#Desired_Vehicle_Operations_Quality">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DirectiveInformationContentEntity"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/eloui/ontologies/2022/1/VehOps#logically_decomposes_into"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/eloui/ontologies/2022/1/VehOps#Physical_Quantity_Specification"/>
                    </owl:Restriction>
                    <owl:Restriction>
